import java.util.*;

class Main
{
    public static void main(String args[])
    {
        //Fill your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for(int i=1;i<=n;i++){
            for(int j=n; j>=i;j--){
                System.out.print( j+" " );
            }
            System.out.println();
        }
    }
}
