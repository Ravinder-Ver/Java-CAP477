import java.util.*;

class Main
{
    public static void main(String args[])
    {
        //Fill your code here
        Scanner sc = new Scanner(System.in);
        int n;
        n = sc.nextInt();
        int res=1;
        for(int i=1;i<=n;i++){
            res=res*i;
        }
            System.out.println(res);
        }
    
}
